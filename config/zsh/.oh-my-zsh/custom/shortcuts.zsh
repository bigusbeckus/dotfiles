# Shortcuts to commonly used folders
personal="~/Documents/Personal"
work="~/Documents/Work"
nvimconf="~/.config/nvim"
zshconf="~/.oh-my-zsh/custom"
alacrittyconf="~/.config/alacritty"

# Cheeky aliases
alias personal="cd $personal"
alias work="cd $work"
alias nvimconf="cd $nvimconf"
alias zshconf="cd $zshconf"
alias alacrittyconf="cd $alacrittyconf"
